{
  "name": "slpb",
  "version": "0.2.4-beta",
  "server_version": "0.0.2-beta",
  "date": "2024-08-24",
  "description": "Список серверов Counter Strike 2",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production electron .",
    "buildWinP": "cross-env NODE_ENV=production electron-packager . slpb --platform=win32 --arch=x64 --icon=./assets/favicon.ico --prune=true --out=dist",
    "buildWinD": "cross-env NODE_ENV=development electron-packager . slpb --platform=win32 --arch=x64 --icon=./assets/favicon.ico --prune=true --out=dist",
    "create-latest-yml": "node createLatestYml.js",
    "create-latest-json": "node createLatestJson.js",
    "dist": "npm run buildWinP && npm run create-latest-yml",
    "generate-blockmap": "node generateBlockmap.js",
    "generate-checksum": "node generateChecksum.js"
  },
  "build": {
    "appId": "com.asqdanil.slpb",
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "AsQqqq",
        "repo": "server-list-play-better",
        "releaseType": "release"
      }
    ],
    "files": [
      "**/*"
    ]
  },
  "author": "AsQ (Danila)",
  "license": "ISC",
  "dependencies": {
    "electron-log": "^5.1.7",
    "electron-updater": "^6.2.1",
    "node-fetch": "^3.3.2",
    "path-browserify": "^1.0.1",
    "update-electron-app": "^3.0.0",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "electron": "^32.0.1",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "electron-winstaller": "^5.4.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}